{
 "name": "yep/tracy-twig-extensions",
 "description": "Twig extensions for Tracy Debugger",
 "keywords": [
  "twig",
  "tracy",
  "debug"
 ],
 "homepage": "https://github.com/YepFoundation/tracy-twig-extensions",
 "minimum-stability": "stable",
 "license": [
  "BSD-3-Clause",
  "GPL-2.0",
  "GPL-3.0"
 ],
 "authors": [
  {
   "name": "Martin Zeman (Zemistr)",
   "homepage": "http://zemistr.eu"
  }
 ],
 "require": {
   "php": ">=5.6.0",
   "tracy/tracy": "^2.4",
   "twig/twig": "^1.31|^2.0"
 },
 "require-dev": {
   "phpunit/phpunit": "5.4.*",
   "yep/reflection": "^1.0"
 },
  "config": {
    "platform": {
      "php": "5.6.0"
    }
  },
 "autoload": {
  "classmap": [
   "src/"
  ]
 }
}
